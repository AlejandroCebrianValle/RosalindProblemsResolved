### Binary search function
binarySearch <- function(arraySearch, item){
lowValue <- 0;
maxValue <- arraySearch.size() - 1;
index <- -1;
while (lowValue <= maxValue){
mid = (lowValue + maxValue) %/% 2;
if (item == arraySearch[mid]){
index <- mid;
break;
} else {
if (item < arraySearch[mid]){
maxValue <- mid - 1;
} else {
lowValue <- mid + 1;
}
}
return(index);
}
}
